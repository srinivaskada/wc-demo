{
  "name": "wc-demo",
  "version": "0.0.1",
  "description": "Stencil Component Starter",
  "main": "dist/index.cjs.js",
  "module": "dist/index.js",
  "es2015": "dist/esm/index.mjs",
  "es2017": "dist/esm/index.mjs",
  "types": "dist/types/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/wc-demo/wc-demo.esm.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/ionic-team/stencil-component-starter.git"
  },
  "files": [
    "dist/",
    "loader/"
  ],
  "scripts": {
    "postinstall": "npm run build:prod2",
    "build": "stencil build --docs",
    "build:dev2": "SURF_API_ENVIRONMENT=dev2 stencil build --docs",
    "build:stage2": "SURF_API_ENVIRONMENT=stage2 stencil build --docs",
    "build:prod2": "SURF_API_ENVIRONMENT=prod2 stencil build --docs",
    "start": "stencil build --dev --watch --serve",
    "start:dev2": "SURF_API_ENVIRONMENT=dev2 LOCAL=true stencil build --dev --watch --serve",
    "start:stage2": "SURF_API_ENVIRONMENT=stage2 LOCAL=true stencil build --dev --watch --serve",
    "start:prod2": "SURF_API_ENVIRONMENT=prod2 LOCAL=true stencil build --dev --watch --serve",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "generate": "stencil generate"
  },
  "dependencies": {
    "@material-icons/svg": "^1.0.27",
    "@stencil/core": "^2.13.0",
    "adapterjs": "^0.15.5",
    "axios": "^0.27.2",
    "bulma": "^0.9.4",
    "hls.js": "^1.1.5",
    "stencil-inline-svg": "^1.1.0"
  },
  "devDependencies": {
    "@stencil/sass": "^1.5.2",
    "@types/jest": "^27.0.3",
    "jest": "^27.4.5",
    "jest-cli": "^27.4.5",
    "puppeteer": "^10.0.0"
  },
  "license": "MIT"
}
